  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Server",
      "request": "launch",
      "command": "yarn dev",
      "skipFiles": ["<node_internals>/**"],
      "type": "node-terminal"
    },
    {
      "name": "Start Resources",
      "request": "launch",
      "command": "docker-compose up",
      "type": "node-terminal"
    },
    {
      "name": "Start Prisma Studio",
      "request": "launch",
      "command": "yarn prisma studio",
      "type": "node-terminal"
    }
  ],
  "compounds": [
    {
      "name": "Develop",
      "configurations": [
        "Start Resources",
        "Start Prisma Studio",
        "Start Server"
      ]
    }
  ]
}
